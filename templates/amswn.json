{
  "id": "htcondor-executor-ams",
  "args": ["-e", "{{ams_config_cm_ip}}"],
  "cpus": 1,
  "mem": 2000.0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "{{ams_config_condor_image}}",
      "forcePullImage": true,
      "privileged": true,
      "network": "BRIDGE"
    },
    "volumes": [{
        "containerPath": "/cvmfs",
        "hostPath": "/cvmfs",
        "mode": "RW"
    }]
  }
}
